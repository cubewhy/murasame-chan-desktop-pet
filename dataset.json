    {
        "character": "丛雨",
        "text": "「盯——……」"
    },
    {
        "character": "将臣",
        "text": "「嗯？」"
    },
    {
        "character": "独白",
        "text": "我感觉身后有一股强烈的视线在盯着我。"
    },
    {
        "character": "独白",
        "text": "回过头。"
    },
    {
        "character": "丛雨",
        "text": "「我闪」"
    },
    {
        "character": "独白",
        "text": "但是，没人。"
    },
    {
        "character": "独白",
        "text": "多心了嘛？"
    },
    {
        "character": "廉太郎",
        "text": "「怎么啦？将臣」"
