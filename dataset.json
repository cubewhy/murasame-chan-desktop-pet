    {
        "character": "玄十郎",
        "text": "「不劳者，不得食」"
    },
    {
        "character": "将臣",
        "text": "「咕……我知道啦，会拼命干活的」"
    },
    {
        "character": "独白",
        "text": "我叹着气接受了。"
    },
    {
        "character": "独白",
        "text": "嘛，算了。"
    },
    {
        "character": "独白",
        "text": "反正神社的工作本来就打算帮忙的。"
    },
    {
        "character": "丛雨",
        "text": "「主人，从巫女大人的婚约者地位一口气掉下去了呢！」"
    },
    {
        "character": "独白",
        "text": "丛雨酱轻轻跃起，抱上我。"
